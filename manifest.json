{
    "manifest_version": 3,
    "name": "Google Search Keyboard Navigator",
    "version": "0.1",
    "description": "Navigate Google search results with up/down arrows and Enter.",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.google.com/search*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}